# 交易代理 HTTP API 地址 (集成服务器)
HTTP_API_URL=http://localhost:3000

# 请求超时时间 (毫秒)
REQUEST_TIMEOUT=30000

# 日志级别 (error=错误, warn=警告, info=信息, debug=调试)
LOG_LEVEL=info

# 买入参数
BUY_AMOUNT_SOL=0.01               # 我们的买入金额 (SOL)
SLIPPAGE_BPS=500                  # 滑点保护 (基点, 500=5%)
BUY_INTERVAL=60                   # 买入间隔（秒）

# 机器人钱包地址
BOT_ADDRESS=xxx

# Unix socket路径,用于接收订阅数据（需要与服务端保持一致）
UNIX_SOCKET_PATH=/tmp/parser_proxy.sock

# reids连接地址，根据系统的redis正确填写
REDIS_URL=unix:///home/redis-run/redis.sock?db=2

# Solana RPC节点地址
RPC_URL=xxx

# 地址查找表账户
LOOKUP_TABLE_ACCOUNTS=xxx

# Nonce账户的公钥
NONCE_PUBKEY=xxx

# 是否启用Nonce账户功能
USE_NONCE=false

# 高低小费策略开关
ENABLE_HIGH_LOW_FEE=false      # 是否启用高低小费策略 (默认: false)

# Gas Fee 配置 - 控制所有SWQOS提供商的费用设置
GLOBAL_CU_LIMIT=1100000        # 计算单元限制 (默认: 1100000)
GLOBAL_CU_PRICE=180000         # 计算单元价格 (默认: 180000)
GLOBAL_BUY_TIP=0.0001          # 买入小费 SOL
GLOBAL_SELL_TIP=0.0001         # 卖出小费 SOL

# 低小费高优先费策略 (快速执行，低成本)
HIGH_CU_PRICE=500000          # 高优先费价格 
LOW_BUY_TIP=0.0001            # 低买入小费 SOL
LOW_SELL_TIP=0.0001           # 低卖出小费 SOL

# 高小费低优先费策略 (高成功率)
LOW_CU_PRICE=180000           # 低优先费价格
HIGH_BUY_TIP=0.0002           # 高买入小费 SOL
HIGH_SELL_TIP=0.0002          # 高卖出小费 SOL
